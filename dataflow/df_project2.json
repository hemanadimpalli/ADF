{
	"name": "df_project2",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Ds_dl_temporaryBronze",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "window1"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "filter1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          ProductId as string,",
				"          Price as string,",
				"          guid as string,",
				"          StartDate as string,",
				"          EndDate as string,",
				"          CreateDate as string,",
				"          ModifiedDate as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 window(over(ProductId,",
				"          Price,",
				"          StartDate,",
				"          EndDate,",
				"          CreateDate,",
				"          ModifiedDate),",
				"     RowCount = count()) ~> window1",
				"window1 derive(Duplicate = iif(toInteger(RowCount)>1, true(), false())) ~> derivedColumn1",
				"derivedColumn1 filter(Duplicate==true()) ~> filter1"
			]
		}
	}
}