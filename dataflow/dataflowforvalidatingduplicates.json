{
	"name": "dataflowforvalidatingduplicates",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Ds_dl_inconingfileProductCSV",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "aggregate1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          ProductId as string,",
				"          Price as string,",
				"          guid as string,",
				"          StartDate as string,",
				"          EndDate as string,",
				"          CreateDate as string,",
				"          ModifiedDate as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 aggregate(groupBy(ProductId),",
				"     count = count(ProductId)) ~> aggregate1"
			]
		}
	}
}